#include "holberton.h"
#include <stdio.h>

/**
 *main: entry point
 *description: print 9 timestable
 *return: the values
 */

void times_table(void)
{
	int i, num;
	num = 9;

	for (i=0; i<=9; i++)
	{
		printf("%d * %d '=' %d\n", num,  i, (i * num));
	}
	return 0;
}

int main(void)
{
	times_table();
	return (0);
}
