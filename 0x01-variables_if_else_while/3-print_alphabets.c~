#include<stdio.h>

/**
 *main: entry point
 *description a program that prints lowercase then uppercase
 *return 0
 */

int main(void)
{
  char letter = 'a';
  char lettertwo = 'A';

  while (letter <= 'z' && lettertwo <= 'Z')
  {
      putchar(letter);
      letter++;
      putchar(lettertwo);
      lettertwo++;
  }
  putchar('\n');
  return (0);
}
